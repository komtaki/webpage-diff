#!/bin/bash

echo compare...

#画像比較を実行 PC用
for i in {1..21} ; do
    compare ./images/pc_${i}_compare.png ./images/pc_${i}_original.png ./images/diff_pc_${i}.png
done

for i in {1..21} ; do
    compare ./images/sp_${i}_compare.png ./images/sp_${i}_original.png ./images/diff_sp_${i}.png
done

echo Finished!!! Plese Check 'images' directory!